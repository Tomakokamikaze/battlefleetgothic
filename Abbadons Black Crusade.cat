                  <profiles>
                    <profile id="b574-53e8-3b27-38c0" name="Chaos Lord" hidden="false" typeId="436f6d6d616e64657223232344415441232323" typeName="Commander">
                      <modifiers>
                        <modifier type="set" field="52652d726f6c6c7323232344415441232323" value="1">
                          <conditions>
                            <condition field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" childId="32dd-e504-db60-3d40" type="equalTo"/>
