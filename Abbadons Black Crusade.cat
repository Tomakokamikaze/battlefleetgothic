    <selectionEntryGroup id="e191-54b6-3066-cbca" name="Ordnance" hidden="false" collective="false" defaultSelectionEntryId="179e-1784-e513-81f1">
      <constraints>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="b9d0-1bd0-33e8-fffc" type="min"/>
        <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="0b3d-302e-e7aa-91a9" type="max"/>
